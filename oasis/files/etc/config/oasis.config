config basic 'assist'
	option enable '1'
#	option rag '1'

config storage 'storage'
	option path '/etc/oasis/chat_data'
	option prefix 'chat-'
	option chat_max '30'

config rollback 'rollback'
	option confirm '0'
	option time '300' # 300s (default)

config sysrole 'role'
	option path '/etc/oasis/oasis.conf'
	option chat '0'
	option prompt '0'
	option call '0'

config debug 'debug'
	option disabled '1'
	option volatile '1'
# volatile '1' ---> /tmp/xxx.log
# volatile '0' ---> /etc/oasis/xxx.log